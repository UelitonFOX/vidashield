{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "no-unused-vars": "warn"
  },
  "overrides": [
    {
      "files": ["**/*.{js,jsx,ts,tsx}"],
      "rules": {
        "react/no-unknown-property": ["error", { "ignore": ["class", "className", "tw"] }]
      }
    }
  ]
} 